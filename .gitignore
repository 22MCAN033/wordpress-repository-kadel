# Wordpress - ignore core, configuration, examples, uploads and logs.
# https://github.com/github/gitignore/blob/main/WordPress.gitignore

# Core
#
# Note: if you want to stage/commit WP core files
# you can delete this whole section/until Configuration.
wordpress-core/
website/vendor/

# Configuration
#wp-config.php

# Uploads
website/app/uploads/

# All plugins
#
# Note: If you wish to whitelist plugins,
# uncomment the next line
#/wp-content/plugins
website/app/plugins/

# W3 Total Cache plugin
website/app/w3tc-config
website/app/cache
website/app/advanced-cache.php

# All themes
#
# Keep customs ones and ignore others
website/app/themes/*
!website/app/themes/rapkalin-wp

# Log files
*.log

# htaccess
/.htaccess

# Defaut files
.idea/
.env
composer.lock